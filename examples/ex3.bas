'
SUB Main
    LET a = 4

    IF a > 0 THEN
        PRINT "Ok"
    END IF

    IF a = 7 THEN
        PRINT "seven"
    ELSE
        PRINT "not seven"
    END IF

    IF a >= 12 THEN
        PRINT "A"
    ELSEIF a <= 13 THEN
        PRINT "B"
    ELSE
        PRINT "X"
    END IF

    IF a <> 777 THEN
        PRINT "11"
    ELSEIF a > 888 THEN
        PRINT "22"
    ELSEIF a < 999 THEN
        PRINT "33"
    END IF
END SUB
